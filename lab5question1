#include <iostream>
using namespace std;
void func(int array[],int i)
{int sum=0;
for(int j=0;j<i;j++)
{sum+=array[j];}
cout<<"\nSum of all elements of the array is - "<<sum;
}

int main(){
int array[20],i,j;
cout<<"Enter how many elements you want in your array (max 20)";
cin>>j;
cout<<"Enter your numbers";
for(i=0;i<j;i++)
{cin>>array[i];
}
func(array,i);
return 0;
}
